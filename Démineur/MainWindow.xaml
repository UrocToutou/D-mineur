        <Canvas HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="bomberestante__" HorizontalAlignment="Left" Height="100" Margin="200,120,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="bomberestantedizaine" HorizontalAlignment="Left" Height="100" Margin="300,120,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="bomberestanteunité" HorizontalAlignment="Left" Height="100" Margin="400,120,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
